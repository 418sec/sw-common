{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1",
                    "esmodules": true
                },
                "useBuiltIns": "entry",
                "corejs": "3.6.5",
                "bugfixes": true,
                "modules": false,
                "debug":true
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins":[
        "@babel/plugin-transform-arrow-functions"
    ]
}