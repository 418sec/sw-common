{
  "check-coverage": false,
  "clean": true,
  "compact": true,
  "extension": [
    ".ts",
    ".js"
  ],
  "include": [
    "lib/**/*.ts",
    "lib/**/*.js"
  ],
  "instrument": true,
  "preserve-comments": false,
  "reporter": [
    "lcov",
    "text",
    "html"
  ],
  "show-process-tree": false,
  "sourceMap": true
}